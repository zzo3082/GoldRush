@using Test_Login.Models
@using Microsoft.AspNet.Identity
@using Test_Login.Models.Extensions;

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.13/semantic.min.css">
<script src="https://code.jquery.com/jquery-1.10.1.min.js"></script>



<body>
    <div class="ui grid centered doubling stackable">
        <div class="six wide column">
            <div class="ui segment">
                <h1 class="ui header">用戶已訂閱</h1>
                <h3>想要取消訂閱嗎?</h3>
                <form class="ui form" method="post" action="/Manage/SubCancel">
                    <p>點擊下方按鈕來取消訂閱</p>
                    <button id="btnCancel" class="ui button" type="submit">取消訂閱</button>
                </form>

            </div>
        </div>
    </div>

    
</body>

